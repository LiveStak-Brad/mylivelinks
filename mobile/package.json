{
  "name": "mylivelinks-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "postinstall": "node ./scripts/patch-expo-dev-menu.js",
    "prebuild:ios": "node ./scripts/verify-capability-sync-disabled.js",
    "build:ios": "cross-env EXPO_NO_CAPABILITY_SYNC=1 eas build --profile development --platform ios --non-interactive",
    "build:ios:dev": "cross-env EXPO_NO_CAPABILITY_SYNC=1 eas build --profile development --platform ios --non-interactive",
    "build:ios:preview": "cross-env EXPO_NO_CAPABILITY_SYNC=1 eas build --profile preview --platform ios --non-interactive",
    "build:ios:prod": "cross-env EXPO_NO_CAPABILITY_SYNC=1 eas build --profile production --platform ios --non-interactive",
    "build:android": "eas build --profile development --platform android --non-interactive",
    "build:android:prod": "eas build --profile production --platform android --non-interactive"
  },
  "dependencies": {
    "@livekit/react-native": "^2.0.2",
    "@livekit/react-native-webrtc": "^114.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.7.0",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@supabase/supabase-js": "^2.38.0",
    "expo": "~50.0.0",
    "expo-apple-authentication": "^8.0.8",
    "expo-av": "~13.10.6",
    "expo-blur": "~12.9.0",
    "expo-camera": "~14.1.3",
    "expo-clipboard": "~5.0.1",
    "expo-dev-client": "~3.3.12",
    "expo-document-picker": "~11.10.1",
    "expo-file-system": "~16.0.7",
    "expo-image-picker": "~14.7.1",
    "expo-keep-awake": "~12.8.2",
    "expo-screen-orientation": "~6.4.1",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.13",
    "livekit-client": "^2.0.0",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-iap": "13.0.4",
    "react-native-image-filter-kit": "^0.7.3",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "13.6.4",
    "text-encoding": "^0.7.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "cross-env": "^10.1.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
