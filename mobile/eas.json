{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "macos-sequoia-15.6-xcode-16.4",
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_DEBUG_LIVE": "1",
        "EXPO_PUBLIC_API_URL": "https://mylivelinks.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "image": "macos-sequoia-15.6-xcode-16.4",
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "env": {
        "EXPO_PUBLIC_DEBUG_LIVE": "0",
        "EXPO_PUBLIC_API_URL": "https://mylivelinks.com"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "image": "macos-sequoia-15.6-xcode-16.4",
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "env": {
        "EXPO_PUBLIC_DEBUG_LIVE": "0",
        "EXPO_PUBLIC_API_URL": "https://mylivelinks.com"
      }
    }
  },
  "submit": {
    "development": {
      "ios": {}
    },
    "preview": {
      "ios": {}
    },
    "production": {
      "ios": {}
    }
  }
}

